  "scripts": {
    "clean": "rimraf dist/* .tmp/*",
    "start": "node server",
    "start:client": "npm run webpack:start",
    "start:server:debug": "nodemon %NODE_DEBUG_OPTION% index -w server -d 2",
    "start:server": "nodemon index -w server -d 2",
    "start:dev": "npm run open -s & parallelshell \"npm start:server -s\" \"npm run start:client -s\"",
    "server:build": "npm run server:lint -s && npm run server:transpile -s && npm run server:json -s",
    "server:lint": "eslint server/**/*.js",
    "server:transpile": "babel server -d dist/server --presets es2015",
    "build:all": "npm run clean -s && npm run server:build -s && npm run webpack:build -s && npm version patch",
    "build:dev": "npm run clean -s && npm run server:build -s",
    "setup": "yarn install",
    "test": "karma start karma.config.js --single-run",
    "test:server": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "open": "opener http://localhost:8000",
    "watch": "npm run watch:server -s",
    "watch:server": "onchange server/**/*.js -- npm run server:build",
    "watch:test": "karma start karma.config.js --auto-watch",
    "webpack:build": "webpack --bail --progress --profile",
    "webpack:start": "webpack-dev-server --history-api-fallback --progress"
  },
